CXX = cl.exe
CFLAGS = /nologo /O2 /EHsc /I. /D _WIN64 /D _CRT_SECURE_NO_DEPRECATE
TARGET = windows

zz.obj: zz.cpp zz.h
	$(CXX) $(CFLAGS) -c zz.cpp

lib: zz.cpp zz.h zz.def tron.obj
	$(CXX) $(CFLAGS) -LD zz.cpp -Fe$(TARGET)\libzz -link -DEF:zz.def

clean:
	 -erase /Q *.obj $(TARGET)\*.exe $(TARGET)\*.dll $(TARGET)\*.exp $(TARGET)\*.lib
